<?xml version="1.0" encoding="utf-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-esaylink" version="0.0.1">
  <name>EsayLinkPlugin</name>
  <description>易联支付插件，esaylink payment</description>
  <keywords>esaylink, payment</keywords>
  <preference name="PAY_URL"/>
  <preference name="NOTIFY_URL"/>
  <preference name="ENVIRONMENT"/>
  <js-module name="EsayLinkPlugin" src="www/EsayLinkPlugin.js">
    <clobbers target="EsayLinkPlugin"/>
  </js-module>
  <platform name="android">
    <config-file parent="/*" target="res/xml/config.xml">
      <preference name="payUrl" value="$PAY_URL"/>
      <preference name="notifyUrl" value="$NOTIFY_URL"/>
      <preference name="environment" value="$ENVIRONMENT"/>
      <feature name="EsayLinkPlugin">
        <param name="android-package" value="hhmao.plugin.esaylink.EsayLinkPlugin"/>
        <param name="onload" value="true"/>
      </feature>
    </config-file>
    <source-file src="src/android/libs/locSDK_4.2.jar" target-dir="libs"/>
    <source-file src="src/android/libs/org.apache.http.legacy.jar" target-dir="libs"/>
    <source-file src="src/android/libs/payecoplugin.jar" target-dir="libs"/>
    <source-file src="src/android/libs/armeabi/liblocSDK4d.so" target-dir="libs/armeabi"/>
    <source-file src="src/android/EsayLinkPlugin.java" target-dir="src/hhmao/plugin/esaylink"/>
    <source-file src="src/android/layout/payeco_plugin_credit_keyboard.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_credit_keyboard_land.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_hxkeyboard.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_hxkeyboard_land.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_keyboard.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_keyboard_land.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_record.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_spinner_itme.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_vedio.xml" target-dir="res/layout"/>
    <source-file src="src/android/layout/payeco_plugin_wait_dialog.xml" target-dir="res/layout"/>
    <source-file src="src/android/values/payeco_colors.xml" target-dir="res/values"/>
    <source-file src="src/android/values/payeco_dimens.xml" target-dir="res/values"/>
    <source-file src="src/android/values/payeco_strings.xml" target-dir="res/values"/>
    <source-file src="src/android/values/payeco_styles.xml" target-dir="res/values"/>
    <source-file src="src/android/drawable-hdpi/payeco_btnenable.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_keyboard_btn_selector.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_keyboard_red_bg.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_keyboard_toast_bg.9.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_back.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_bomarr.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_btnleft_selector.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_btnright_selector.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_editbg.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_progressbar.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_rightarr.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_spinner_bg.9.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_spinner_bg_on.9.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_spinner_selector.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_plugin_topicon.png" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_radiu_dialog.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_stand_btnselector.xml" target-dir="res/drawable-hdpi"/>
    <source-file src="src/android/drawable-hdpi/payeco_stand_digtselector.xml" target-dir="res/drawable-hdpi"/>
    <config-file parent="/manifest" target="AndroidManifest.xml">
      <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
      <uses-permission android:name="android.permission.INTERNET"/>
      <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
      <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
      <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
      <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
      <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
      <uses-permission android:name="android.permission.RECORD_AUDIO"/>
      <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
      <uses-feature android:name="android.hardware.camera"/>
      <uses-permission android:name="android.permission.CAMERA"/>
      <uses-feature android:name="android.hardware.camera.autofocus"/>
      <uses-permission android:name="android.permission.RECEIVE_SMS"/>
      <uses-permission android:name="android.permission.READ_SMS"/>
      <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
      <uses-permission android:name="android.permission.RECORD_VIDEO"/>
    </config-file>
    <config-file parent="/manifest/application" target="AndroidManifest.xml">
      <activity android:launchMode="singleTask" android:name="com.payeco.android.plugin.PayecoPluginLoadingActivity" android:screenOrientation="portrait"/>
      <activity android:name="com.payeco.android.plugin.PayecoCameraActivity" android:screenOrientation="portrait"/>
      <activity android:name="com.payeco.android.plugin.PayecoVedioActivity" android:process="com.payeco.android.plugin.vedio" android:screenOrientation="landscape"/>
      <service android:enabled="true" android:name="com.baidu.location.f" android:process=":remote">
        <intent-filter>
          <action android:name="com.baidu.location.service_v2.2"/>
        </intent-filter>
      </service>
      <meta-data android:name="com.baidu.lbsapi.API_KEY" android:value="DhNxS47YIr3CNdocGoAcy1ts"/>
    </config-file>
  </platform>
  <platform name="ios">
    
	 <config-file target="config.xml" parent="/widget">
         
      <preference name="selle" value="$PAY_URL"/>
      <preference name="partne" value="$NOTIFY_URL"/>
      <preference name="environment" value="$ENVIRONMENT"/>
      
	 </config-file>
    <config-file  target="config.xml" parent="/*" >
        
      <feature name="EsayLinkPlugin">
        <param name="ios-package" value="EsayLinkPlugin"/>
        <param name="onload" value="true"/>
      </feature>
      
    </config-file>
    
    
    
    <framework src="Security.framework" />
    <framework src="SystemConfiguration.framework" />
    <framework src="CFNetwork.framework" />
    <framework src="QuartzCore.framework" />
    <framework src="CoreGraphics.framework" />
    <framework src="CoreLocation.framework" />
 
    
    <source-file src="src/ios/lib/libPayecoPayPlugin.a" framework="true" />
    
    <header-file src="src/ios/lib/libPayecoPayPlugin.h" />
    <header-file src="src/ios/lib/PPIHudView.h" />
    <header-file src="src/ios/lib/SKYFormRequest.h" />
    
    <header-file src="src/ios/EsayLinkPlugin.h" />
    <source-file src="src/ios/EsayLinkPlugin.m" />

	<!--  
		<header-file src=""/> 
		<framework src="src/ios/lib/"/>   
